pipeline{
    agent any
    
    environment{
        PATH=$PATH:M2_HOME/bin"
    }
    stages{
        stage("git checkout"){
            steps{
                git credentialsId: 'githubp', url: 'https://github.com/stsagar/my-app.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh"Maven clean Packege"
            }
        }
    }
}
